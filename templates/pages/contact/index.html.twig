                        <li><a href="{{ path('app_about') }}">About Us</a></li>
                        <li><a href="{{ path('app_car_details') }}">Car Details</a></li>
                        <li><a href="{{ path('app_blog_details') }}">Blog Details</a></li>
                    </ul>
                </li>
                <li><a href="{{ path('app_about') }}">About</a></li>
                <li><a href="{{ path('contact.index') }}">Contact</a></li>
            </ul>
        </nav>
        <div id="mobile-menu-wrap"></div>
        <ul class="offcanvas__widget__add">
